#include "main.h"
/**
 * env - Function that prints the current environment
 * @n: An array of arguments
 * @m: A double pointer to the beginning of n
 * Return: -1 if it failed, otherwise 0
 */
int env(char **n, char __attribute__((__unused__)) **m)
{
int a;
char b = '\n';
if (!environ)
return (-1);
for (a = 0 ; environ[a] ; a++)
{
write(STDOUT_FILENO, environ[a], _strlen(environ[a]));
write(STDOUT_FILENO, &b, 1);
}
(void)n;
return (0);
}
